import re

pattern = r'\$\d{1,3}(?:,\d{3})*(?:\.\d{1,2})?'

# Test cases
test_strings = [
    "Projected Value at Maturity: $100,00.00",
    "Projected Value at Maturity: $32.0",
    "Projected Value at Maturity: $0.02"
]

for string in test_strings:
    match = re.search(pattern, string)
    if match:
        print(f"Matched value: {match.group()}")
    else:
        print("No match found")

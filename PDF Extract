import pandas as pd
import tabula

# Specify the path to the PDF file
pdf_path = 'path/to/your/pdf_file.pdf'
page_number = 1  # Specify the page number to extract tables from

# Extract all tables from the specified page
tables = tabula.read_pdf(pdf_path, pages=page_number, multiple_tables=True)

# Concatenate all extracted tables into a single DataFrame
final_table = pd.concat(tables, ignore_index=True)

# Display the final table
print(final_table)
